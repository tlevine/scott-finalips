function setItem(pItem,pValue){  
  $('#'+pItem).val(pValue);
  var get = new htmldb_Get(null,340,'APPLICATION_PROCESS=AJAX.returnNothing()',0);  
  get.add(pItem,pValue);  
  gReturn = get.get();  
}  

function getItem(pItem){  
  var get = new htmldb_Get(null,340,'APPLICATION_PROCESS=AJAX.returnItem()',0);  
  get.add('RETURNITEM',pItem);  
  gReturn = get.get();  
  $('#'+pItem).val(gReturn);
  return gReturn;
}

